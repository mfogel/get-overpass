{
  "name": "get-overpass",
  "version": "0.3.0",
  "description": "Get OpenStreetMap objects via the Overpass API as GeoJSON",
  "repository": "github:mfogel/get-overpass",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "test-debug": "node --inspect-brk node_modules/.bin/jest --runInBand"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "bin": {
    "get-overpass": "src/cli.js"
  },
  "keywords": [
    "osm",
    "overpass",
    "geojson"
  ],
  "option-defaults": {
    "api-endpoint": "https://overpass-api.de/api/interpreter",
    "mapbox-ids": false
  },
  "author": "Mike Fogel <mike@fogel.ca>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mfogel/get-overpass/issues"
  },
  "homepage": "https://github.com/mfogel/get-overpass#readme",
  "dependencies": {
    "bluebird": "^3.5.1",
    "query-overpass": "^1.5.3",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "jest": "^21.2.1"
  }
}
